{
    "plugins": [
        "prettier-plugin-astro"
    ],
    "overrides": [
        {
            "files": "*.{js,ts,vue,astro}",
            "options": {
                "parser": "astro",
                "singleQuote": true,
                "semi": true,
                "tabWidth": 4,
                "printWidth": 100
            }
        }
    ]
}
